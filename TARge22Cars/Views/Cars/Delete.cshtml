@model TARge22Cars.Models.Cars.CarsDeleteViewModel

<header>
    <h2>DELETE</h2>
</header>

<body>
    <div>
        <dl class="row">
            <div class="form-group-row">
                <label asp-for="CarId" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.CarId)
            </div>

            <div class="form-group-row">
                <label asp-for="Make" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Make)
            </div>

            <div class="form-group-row">
                <label asp-for="Color" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Color)
            </div>

            <div class="form-group-row">
                <label asp-for="Model" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Model)
            </div>

            <div class="form-group-row">
                <label asp-for="Year" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Year)
            </div>

            <div class="form-group-row">
                <label asp-for="Power" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Power)
            </div>

            <div class="form-group-row">
                <label asp-for="Transmission" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Transmission)
            </div>

            <div class="form-group-row">
                <label asp-for="Fuel" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Fuel)
            </div>

            <div class="form-group-row">
                <label asp-for="FuelConsumption" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.FuelConsumption)
            </div>

            <div class="form-group-row">
                <label asp-for="Mileage" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.Mileage)
            </div>

            <div class="form-group-row">
                <label asp-for="CreatedAt" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.CreatedAt)
            </div>

            <div class="form-group-row">
                <label asp-for="UpdatedAt" class="col-sm-2 col-form-label"></label>
                @Html.DisplayFor(model => model.UpdatedAt)
            </div>
            <form asp-action="DeleteConfirmation">
                <input type="hidden" asp-for="CarId"/>
                <input type="hidden" value="Delete" class="btn"/>
                <a asp-action="Index" asp-controller="cars" class="btn">Back</a>

            </form>
        </dl>
    </div>
</body>